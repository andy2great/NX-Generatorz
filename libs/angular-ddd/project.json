{
  "name": "angular-ddd",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/angular-ddd/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/angular-ddd",
        "main": "libs/angular-ddd/src/index.ts",
        "tsConfig": "libs/angular-ddd/tsconfig.lib.json",
        "assets": [
          "libs/angular-ddd/*.md",
          {
            "input": "./libs/angular-ddd/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/angular-ddd/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/angular-ddd",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/angular-ddd",
            "glob": "collection.json",
            "output": "."
          },
          {
            "input": "./libs/angular-ddd",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/angular-ddd/**/*.ts"]
      }
    }
  },
  "tags": []
}
