{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "DddApi",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the API",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    },
    "domain": {
      "type": "string",
      "description": "The domain to add the API",
      "x-prompt": "What domain would you like to use?"
    }
  },
  "required": [
    "name",
    "domain"
  ]
}