{
  "name": "ddd-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ddd-angular/src",
  "prefix": "nx-generatorz",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{workspaceRoot}/dist/libs/ddd-angular"],
      "options": {
        "outputPath": "dist/libs/ddd-angular",
        "main": "libs/ddd-angular/src/index.ts",
        "tsConfig": "libs/ddd-angular/tsconfig.lib.json",
        "assets": [
          "libs/ddd-angular/*.md",
          {
            "input": "./libs/ddd-angular/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/ddd-angular/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/ddd-angular",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/ddd-angular",
            "glob": "collection.json",
            "output": "."
          },
          {
            "input": "./libs/ddd-angular",
            "glob": "executors.json",
            "output": "."
          }
        ]
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ddd-angular/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ddd-angular/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ddd-angular/src/**/*.ts",
          "libs/ddd-angular/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
